N        write_com(0xc0);/* ]w LCD G} */
N        for(i = 0; i < 16; i++) write_data(' ');
N        write_com(0xc0);
N   }
N   /* gJr */
N    do{ write_data(*str++); }
N    while(*str!='\0');
N}

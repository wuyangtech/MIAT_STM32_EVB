Nvoid print(char line, char *str)
N{	   
N    unsigned char i ;
N    if(line == 1) /* NYM */
N    {
N        write_com(0x80); /* ]w LCD @} */
N        for(i = 0; i < 16; i++) write_data(' ');
N        write_com(0x80);
N    }
N    else
